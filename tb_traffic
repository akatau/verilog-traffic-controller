    %store/vec4 v0x5597ad36f7a0_0, 0, 1;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x5597ad36f840_0, 0, 2;
    %delay 100000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5597ad36f7a0_0, 0, 1;
    %vpi_call 2 40 "$display", "reset done" {0 0 0};
    %delay 200000, 0;
    %load/vec4 v0x5597ad36f6b0_0;
    %cmpi/e 1, 0, 3;
    %jmp/0xz  T_6.0, 4;
    %vpi_call 2 44 "$display", "North is Green" {0 0 0};
T_6.0 ;
    %delay 500000, 0;
    %vpi_call 2 50 "$display", "Car at East now" {0 0 0};
    %pushi/vec4 1, 0, 1;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5597ad36f840_0, 4, 1;
    %delay 2000000, 0;
    %load/vec4 v0x5597ad36f5f0_0;
    %cmpi/e 1, 0, 3;
    %jmp/0xz  T_6.2, 4;
    %vpi_call 2 61 "$display", "East is Green now" {0 0 0};
    %jmp T_6.3;
T_6.2 ;
    %vpi_call 2 62 "$display", "Error: East not green?" {0 0 0};
T_6.3 ;
    %pushi/vec4 1, 0, 1;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5597ad36f840_0, 4, 1;
    %vpi_call 2 66 "$display", "Cars both sides" {0 0 0};
    %delay 2000000, 0;
    %load/vec4 v0x5597ad36f6b0_0;
    %cmpi/e 1, 0, 3;
    %jmp/0xz  T_6.4, 4;
    %vpi_call 2 70 "$display", "Back to North Green" {0 0 0};
T_6.4 ;
    %delay 2000000, 0;
    %load/vec4 v0x5597ad36f5f0_0;
    %cmpi/e 1, 0, 3;
    %jmp/0xz  T_6.6, 4;
    %vpi_call 2 74 "$display", "Back to East Green" {0 0 0};
T_6.6 ;
    %vpi_call 2 76 "$display", "test finished" {0 0 0};
    %vpi_call 2 77 "$stop" {0 0 0};
    %end;
    .thread T_6;
